{
    "name": "kamansoft/klorchid-cms",
    "description": "A simple backoffice cms system build on top of kamansoft/klorchid package",
    "keywords": [
        "kamansoft",
        "klorchid-cms"
    ],
    "homepage": "https://github.com/kamansoft/klorchid-cms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lemys Lopez",
            "email": "lemyskaman@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "kamansoft/klorchid": "dev-lemys_dev",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^1.6",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "orchid/platform": "^9.14"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kamansoft\\KlorchidCms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kamansoft\\KlorchidCms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kamansoft\\KlorchidCms\\KlorchidCmsServiceProvider"
            ],
            "aliases": {
                "KlorchidCms": "Kamansoft\\KlorchidCms\\KlorchidCmsFacade"
            }
        }
    }


}
